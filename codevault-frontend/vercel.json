{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "devCommand": "vite",
    "env": {
        "VITE_API_URL": {
            "description": "Backend API URL"
        }
    },
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "/"
        }
    ]
}